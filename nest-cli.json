{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/agregator/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/agregator/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/agregator",
  "projects": {
    "agregator": {
      "type": "application",
      "root": "apps/agregator",
      "entryFile": "main",
      "sourceRoot": "apps/agregator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agregator/tsconfig.app.json"
      }
    },
    "gateway-service": {
      "type": "application",
      "root": "apps/gateway-service",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-service/tsconfig.app.json"
      }
    },
    "telco-service": {
      "type": "application",
      "root": "apps/telco-service",
      "entryFile": "main",
      "sourceRoot": "apps/telco-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/telco-service/tsconfig.app.json"
      }
    },
    "callback-service": {
      "type": "application",
      "root": "apps/callback-service",
      "entryFile": "main",
      "sourceRoot": "apps/callback-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/callback-service/tsconfig.app.json"
      }
    },
    "management-service": {
      "type": "application",
      "root": "apps/management-service",
      "entryFile": "main",
      "sourceRoot": "apps/management-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/management-service/tsconfig.app.json"
      }
    }
  }
}